{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install",
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/admin",
      "destination": "/"
    },
    {
      "source": "/item/:id",
      "destination": "/"
    }
  ]
}
